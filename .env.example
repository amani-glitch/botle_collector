# Gemini API Key (required)
GEMINI_API_KEY=your_gemini_api_key_here

# Google Sheets — Service Account (required for data export)
# Paste the full JSON content of your service account key file as a single line
GOOGLE_SERVICE_ACCOUNT_KEY={"type":"service_account","project_id":"...","private_key":"...","client_email":"..."}

# Google Sheets — Spreadsheet ID (required for data export)
# Found in the spreadsheet URL: https://docs.google.com/spreadsheets/d/{THIS_IS_THE_ID}/edit
GOOGLE_SHEET_ID=your_spreadsheet_id_here

# Server config (optional)
PORT=3001
CORS_ORIGIN=http://localhost:3000
